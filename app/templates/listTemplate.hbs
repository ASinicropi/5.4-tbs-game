{{#each character}}
  <div class="col-md-3">
    <img class="js-PlayerChar" src="{{image}}" alt="{{name}}" data-character-name="{{name}}"/><br/>
    <h3>{{name}}</h3>
  </div>
{{/each}}

{{#each character}}
  <div class="col-md-3">
    <img class="js-EnemyChar" src="{{image}}" alt="{{name}}" data-heartless-name="{{name}}"/><br/>
    <h3>{{name}}</h3>
  </div>
{{/each}}
